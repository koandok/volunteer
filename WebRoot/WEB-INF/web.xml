<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>volunteer</display-name>
  <welcome-file-list>

    <welcome-file>login.jsp</welcome-file>

  </welcome-file-list>
    <filter>
    <filter-name>CharacterEncodingFilter</filter-name>
    <filter-class>com.gxuwz.volunteer.web.filter.CharacterEncodingFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>CharacterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>ArtServlet</servlet-name>
    <servlet-class>com.gxuwz.volunteer.web.servlet.ArtServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FormServlet</servlet-name>
    <servlet-class>com.gxuwz.volunteer.web.servlet.FormServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.gxuwz.volunteer.web.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>VoServlet</servlet-name>
    <servlet-class>com.gxuwz.volunteer.web.servlet.VoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DepServlet</servlet-name>
    <servlet-class>com.gxuwz.volunteer.web.servlet.DepServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ArtServlet</servlet-name>
    <url-pattern>/ArtServlet</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FormServlet</servlet-name>
    <url-pattern>/FormServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VoServlet</servlet-name>
    <url-pattern>/VoServlet</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>DepServlet</servlet-name>
    <url-pattern>/DepServlet</url-pattern>
  </servlet-mapping>
</web-app>